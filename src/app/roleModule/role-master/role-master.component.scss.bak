  .tbl-section {
    margin-top: 20px;

    .data-body {
      display: flex;
      height: calc(100% - 215px);
      padding: 10px 18px;
      border: 1px solid var(--theme_input_border);
      border-radius: 15px;
      box-shadow: 0px 2px 3px 0px var(--box_shadow);
      background-color: var(--white_color);
      overflow-y: scroll;
      table {
        width: 100%;
        font-size: 12px;
        text-align: center;
        thead {
          tr {
            th {
              font-weight: 600;
              color: var(--theme_input_color);
              font-size: 16px;
              padding: 8px 0;
            }
          }
        }
        
      

       

        tbody {
          color: var(--theme_title);

          tr {
            border-top: 1px solid var(--theme_input_border);
            td {
              padding: 8px 0;
              font-weight: normal;
              font-size: 14px;


            }
            .actions {
              cursor: pointer;

              a+a {
                margin-left: 20px;
              }

              a {
                i {
                  font-size: 14px;

                  &::before {
                    color: var(--theme_bg_blue);
                  }
                }
              }
            }

            .delete {
              cursor: pointer;

              a {
                i::before {
                  font-size: 14px;
                  color: var(--theme_red);
                }
              }
            }
          }
        }

        a {
          color: var(--theme_bg_blue);
        }
      }

    }
  }

  .data-footer {
    padding: 14px 0;
  }

}


// modal design

::ng-deep {
  .modal-header {
    background-color: var(--theme_bg_color);
      border-bottom: 1px solid var(--theme_input_border);
      border-radius: 15px 15px 0px 0px;
  
      h4 {
        font-size: 18px;
        color: var(--theme_black);
        font-weight: 600;
        text-transform: capitalize;
      }
  }

  .modal-body {


    .cust-tbls {
      thead {
        tr {
          display: flex;

          th {
            font-size: 14px;
            color: var(--theme_black);
            font-weight: 600;
            border-right: 1px solid var(--theme_input_border);
            flex: 1;

          }
        }
      }

      tbody {
        tr {
          display: flex;

          td {
            font-size: 12px;
            color: var(--theme_black);
            border-right: 1px solid var(--theme_input_border);
            flex: 1;
            flex-grow: 1;
            flex-basis: 0;
            .mat-checkbox-layout{
              margin-bottom: 0px !important;
            }
        
      

          }
        }
      
      }
    }
    ::ng-deep .mat-form-field-appearance-fill {
      .mat-form-field-infix {
        font-size: 14px;
        padding: 0.25em 0 0.75em 0;
      }

      .mat-form-field-flex {
        border: 0.5px solid var(--profile_icon_border);
        border-radius: 3px;
        color: var(--theme_input_color);
        font-size: 14px;
      }
    }

    .btn-role {
      background-color: var(--theme_bg_blue);
      color: var(--white_color);
      font-size: 16px;
      text-transform: capitalize;
      padding: 10px 30px;
      border-radius: 4px;
      border: 1px solid var(--theme_bg_blue);
      margin-top: 20px;
    }
  }
}
p:hover {
  color: rgb(59, 130, 236);
  background-color: transparent;
  text-decoration: underline;
}

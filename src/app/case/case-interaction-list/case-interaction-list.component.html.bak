<!-- block confiramation modal  -->
<ng-template #blockstatus let-modal>
    <div class="case-info-modal">
        <div class="modal-header">
            <button type="button" class="close" aria-label="Close" (click)="modal.dismiss('Cross click')">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <div class="case_status">

                <div class="caseinfo-modal-bdy">
                    <div class="assign-icon">
                        <i class="icon-block"></i>
                    </div>
                    <h2 class="case-ttl">Are you sure? </h2>
                    <p class="sub-ttl">You want assign to block this user</p>
                </div>
                <div class="case-modal-button">
                    <div class="btn-no">
                        <button (click)="ClosePopup();modal.dismiss('Cross click')">
                            No
                        </button>
                    </div>
                    <div class="btn-yes">
                        <button (click)="modal.dismiss('Cross click');MarkCaseAsBlocked()">
                            Yes
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</ng-template>

<!-- Attachment file List -->

<ng-template #attachmentFilePathList let-modal>
    <div class="assign_modl">
        <div class="modal-header">
            <h4 class="modal-title">Attachments File</h4>
            <button type="button" class="btn" (click)="downloadFile()"
                (click)="modal.dismiss('Cross click')">
              <img src="assets/images/svg/downloads.svg" alt="downloads">
            </button>
            <button type="button" class="close" aria-label="Close" (click)="modal.dismiss('Cross click')">
                <span aria-hidden="true">&times;</span>
            </button>

        </div>
        <div class="modal-body">
            <div class="assign_form ">
                <table class="table">
                    <tr>
                        <th><input type="checkbox" [(ngModel)]="masterSelected" name="list_name" value="m1"
                                (change)="checkUncheckAll()"></th>
                        <th>FileName</th>
                        <th>Size</th>
                        <th>type</th>
                    </tr>
                    <tr *ngFor="let data of fileAttachmentUrl">
                        <td> <input type="checkbox" [(ngModel)]="data.isSelected" name="list_name" value="{{data.id}}"
                                (change)="isAllSelected()" /></td>
                        <td class="ellipsis">{{data.AttachmentFileName}}</td>
                        <td>{{data.AttachmentSize}}</td>
                        <td>{{data.AttachmentFileType}}</td>
                    </tr>


                </table>
            </div>
        </div>
    </div>
</ng-template>

<!-- End attachment -->

<app-notify-modal *ngIf="!showIt"></app-notify-modal>
<app-merge-modal *ngIf="!mergeWith"></app-merge-modal>

<!-- Case Interaction Attachment File  Modal-->


<div class="modal" role="dialog" id="attachmentModal" data-backdrop="static">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content attachement-modal">
            <div class="modal-header">
                <h4 class="modal-title">Attachments</h4>
                <button type="button" class="close" aria-label="Close" (click)="closeModalFile()">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <!-- <div class="">
                    <input type="file" id="file" multiple (change)="getFileDetails($event)" />
                    <label for="file">choose a file</label>
                </div> -->
                <div class="row">
                    <div *ngFor="let file of myFiles" class="col-md-4">
                        <div class="text-center mb-3">
                            <div class="">
                                <img src="assets/images/attachment.png" alt="attachment">
                            </div>
                            <div class="attach-data">
                                <p><span class="info-label">Name:</span>&nbsp;
                                    <span class="data">{{ file.name.substr(0, 5) }}...</span>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="text-center btn-uplode">
                    <div class="mr-3">
                        <input type="file" id="file" multiple (change)="getFileDetails($event)" />
                        <label for="file">choose a file</label>
                    </div>
                    <button class="filled-btn" (click)="uploadFiles()">Upload</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Case Interaction Attachment  Modal -->



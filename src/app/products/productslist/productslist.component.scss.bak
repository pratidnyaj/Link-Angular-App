.wrapper {
    width: calc(100% - 80px);
    position: relative;
    left: 75px;
    top: 95px;
  
    .action-perform-btns {
      display: flex;
      justify-content: space-between;
      flex: 1;
  
      .search-actions {
        display: flex;
        align-items: center;
  
        .show-count {
          display: flex;
          align-items: center;
  
  
          span {
            padding-right: 6px;
            color: var (--theme_black);
            font-size: 14px;
          }
  
          .select-dropdown {
            position: relative;
            background-color: var(--white_color);
            border-radius: 3px;
            border: 1px solid var(--theme_input_border);
            padding: 6px;
            //   box-shadow: 2px 3px var(--box_shadow);
            margin-right: 5px;
  
  
            select {
              font-size: 1rem;
              font-weight: normal;
              max-width: 100%;
              padding: 0px 16px 0px 4px;
              border: none;
              font-size: 14px;
              color: var(--theme_input_colo);
              background-color: transparent;
              -webkit-appearance: none;
              -moz-appearance: none;
              appearance: none;
  
              &:active {
                outline: none;
                box-shadow: none;
              }
  
              &:focus {
                outline: none;
                box-shadow: none;
              }
            }
  
            &:after {
              content: "";
              position: absolute;
              top: 50%;
              right: 8px;
              width: 0;
              height: 0;
              margin-top: -2px;
              border-top: 5px solid var(--theme_black);
              border-right: 5px solid transparent;
              border-left: 5px solid transparent;
            }
          }
        }
  
        .search {
          position: relative;
          margin-left: 20px;
  
          input {
            background-color: var(--white_color);
            box-shadow: 0px 2px 3px var(--box_shadow);
            border: 1px solid var(--theme_input_border);
            border-radius: 3px;
            padding: 4px 6px;
            color: var(--theme_input_color);
            width: 250px;
  
            &::placeholder {
              color: var(--theme_input_color);
              font-size: 12px;
            }
  
            &:focus {
              outline: none;
            }
          }
  
          .find {
            background-color: transparent;
            border: none;
            position: absolute;
            right: 6px;
            top: 6px;
  
            &:focus {
              outline: none;
              box-shadow: none;
            }
          }
        }
      }
  
      .filter-action {
        display: flex;
        align-items: center;
        justify-content: space-between;
  
        .filters {
          margin-right: 16px;
          border: 1px solid var(--theme_input_border);
          box-shadow: 0px 2px 3px var(--box_shadow);
          border-radius: 3px;
          background-color: var(--white_color);
          padding: 6px 22px;
          font-size: 14px;
          color: var(--theme_input_color);
  
          span {
            i {
              margin-right: 6px;
            }
          }
  
          &:last-child {
            margin-right: 0px;
          }
        }
        .filter-blue{
            background-color:var(--theme_bg_blue);
            color: var(--white_color);
        }
      }
    }
  
    .tbl-section {
      margin-top: 20px;
  
      .data-body {
        display: flex;
        height: calc(100% - 105px);
        padding: 10px 18px;
        border: 1px solid var(--theme_input_border);
        border-radius: 15px;
        box-shadow: 0px 2px 3px 0px var(--box_shadow);
        background-color: var(--white_color);
  
        table {
          width: 100%;
          font-size: 12px;
          text-align: center;
  
          th,
          td {
            padding: 8px 0;
            font-weight: normal;
          }
          th
          {
            font-weight: bold;
          }
  
          thead {
            color: var(--theme_input_color);
          }
  
          tbody {
            color: var(--theme_title);
  
            tr {
              border-top: 1px solid var(--theme_input_border);
  
              .actions {
                  cursor: pointer;
                a+a {
                  margin-left: 20px;
                }
  
                a {
                  i {
                    font-size: 14px;
  
                    &::before {
                      color: var(--theme_bg_blue);
                    }
                  }
                }
              }
  
              .delete {
                  cursor: pointer;
  
                a {
                  i::before {
                    font-size: 14px;
                    color: var(--theme_red);
                  }
                }
              }
            }
          }
  
          a {
            color: var(--theme_bg_blue);
          }
        }
  
      }
    }
  
    .data-footer {
      padding: 14px 0;
    }
    // .tbl-footer {
    //   display: flex;
    //   align-items: center;
    //   justify-content: space-between;
    //   padding: 20px;
    //   .displaying_count {
    //     color: var(--theme_title);
    //     font-size: 12px;
    //   }
    // }
    
  }
  

  // product modal design start /
  .product-modal{
    .prod-header{
      background-color: var(--theme_bg_color);
      border-bottom: 1px solid var(--theme_input_border);
      border-radius: 15px 15px 0px 0px;
  
      h4 {
        font-size: 18px;
        color: var(--theme_black);
        font-weight: 600;
        text-transform: capitalize;
      }
    }
    .prod-body{
      padding: 40px ;
      .filled-btn {
        border: 1px solid var(--theme_bg_blue);
        color: var(--white_color);
        font-size: 14px;
        font-weight: bold;
        padding: 10px 25px;
        background-color: var(--theme_bg_blue);
        border-radius: 3px;
        cursor: pointer;
        // margin-bottom: 15px;
        // width: 45%;
        margin: 0 auto;

        &:hover {
          box-shadow: 0 2px 4px -1px rgb(0 0 0 / 20%), 0 4px 5px 0 rgb(0 0 0 / 14%),
            0 1px 10px 0 rgb(0 0 0 / 12%);
        }
      }
    }
  }

















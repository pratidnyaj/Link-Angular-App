.wrapper {
    height: calc(100vh - 75px);
    display: flex;
    position: relative;
    left: 55px;
    top: 72px;
    .left-side-panel {
        position: relative;
        z-index: 2;
        box-shadow: 0px 2px 3px 0px var(--box_shadow);
        flex: 0 0 320px;
        background-color: var(--white_color);
        overflow-y: scroll;
        .custaccordian_header {
            display: flex;
            align-items: center;
            justify-content: space-between;
            .acc_title {
                .case_level {
                    margin-right: 10px;
                }
            }
        }
        ::ng-deep .accordion {
            .card {
                .card-header {
                    padding: 0px;
                    background-color: var(--white_color);
                    button {
                        padding: 15px;
                        width: 100%;
                        color: var(--theme_title);
                        font-weight: 600;
                        .custaccordian_header {
                            .acc_add {
                                transform: rotate(135deg);
                                transition: 1s all;
                            }
                        }
                        &:hover {
                            text-decoration: none;
                        }
                        &:focus {
                            outline: none;
                            box-shadow: none;
                            text-decoration: none;
                        }
                    }
                    button.collapsed {
                        .custaccordian_header {
                            .acc_add {
                                transform: rotate(0deg);
                                transition: 1s all;
                            }
                        }
                    }
                }
                .card-body {
                    background-color: var(--theme_input_border);
                }
            }
        }
    }
    .middle-panel-content {
        flex: 1;
        overflow: hidden;
        .cust-mid {
            margin: 15px;
            // // width: 93%;
            width: calc(100% - 70px);
        }
    }
}

